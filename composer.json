{
    "name": "mnrahimi/browsine",
    "type": "library",
    "description": "Minimalistic PHP web browser.",
    "keywords": ["library", "browsine", "browser", "web browser"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mnrahimi/browsine/issues",
        "source": "https://github.com/mnrahimi/browsine"
    },
    "authors": [
        {
            "name": "Muhammad Nasir Rahimi",
            "email": "mnr@karyal.com"
        }
    ],
    "require": {
